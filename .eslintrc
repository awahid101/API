{
  "env": {
    "node": true
  },
  "parserOptions": {
      "ecmaVersion": 6
  },
  "rules": {
    "no-use-before-define": 2,
    "camelcase": 0,
    "no-unused-vars": [2, {"args":"none"}],
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-shadow": 0,
    "keyword-spacing": [2, {"after": true}],
    "space-before-blocks": [2, "always"]
  }
}
